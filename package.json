{
  "name": "@ctrl/cloudflare",
  "version": "0.0.0-placeholder",
  "description": "Bypass Cloudflare's anti-bot page also known as I'm Under Attack Mode",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "typectrl/cloudflare",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "cloudflare",
    "got",
    "bypass",
    "jschl_vc",
    "typescript"
  ],
  "main": "src/index.js",
  "typings": "src/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "lint": "tslint -t stylish -p .",
    "lint:fix": "tslint -p tsconfig.json -t stylish --fix 'src/**/*.ts' 'test/**/*.ts'",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "postbuild": "cp package.json dist && cp LICENSE dist && cp README.md dist && rimraf dist/test",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --no-cache",
    "semantic-release": "cd dist && semantic-release"
  },
  "dependencies": {
    "@types/got": "^9.4.0",
    "delay": "^4.1.0",
    "got": "^9.6.0",
    "ua-string": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "23.3.13",
    "@types/nock": "9.3.1",
    "@types/node": "10.12.19",
    "@types/tough-cookie": "2.3.5",
    "@types/ua-string": "2.0.0",
    "jest": "23.6.0",
    "nock": "10.0.6",
    "prettier": "1.16.3",
    "rimraf": "2.6.3",
    "semantic-release": "15.13.3",
    "tough-cookie": "3.0.0",
    "ts-jest": "23.10.5",
    "ts-node": "7.0.1",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.17.0",
    "tslint-xo": "0.11.0",
    "typescript": "3.3.1"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
